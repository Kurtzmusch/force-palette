{

  "manifest_version": 2,
  "name": "force-palette",
  "version": "1.0",

  "description": "forces a color palette on webpages",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["hide-body.css"],
      "js": ["colors.js", "force-palette.js"],
      "run_at": "document_start"
    }
  ]

}

